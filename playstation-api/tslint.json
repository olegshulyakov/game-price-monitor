{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "max-line-length": [true, 250],
    "quotemark": [true, "double", "jsx-double"],
    "indent": [
      true,
      "spaces",
      4
    ],
    "ter-indent": false
  },
  "rulesDirectory": []
}